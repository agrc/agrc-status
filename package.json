{
  "name": "netlify-statuskit",
  "version": "1.0.0",
  "description": "StatusKit is a template to build Status pages for your business",
  "main": "gulpfile.mjs",
  "type": "module",
  "scripts": {
    "build": "hugo --source site --gc --minify",
    "build-preview": "hugo --source site --gc --minify --buildDrafts --buildFuture --logLevel debug",
    "start": "gulp server",
    "lint": "eslint src gulpfile.mjs",
    "new-incident": "gulp newIncident"
  },
  "author": "UGRC",
  "license": "MIT",
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "del": "^7.1.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "inquirer": "^9.2.11",
    "lodash.kebabcase": "^4.1.1",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "tomlify-j0.4": "^3.0.0",
    "tomljs": "^0.1.3"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "es2024": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "globals": {
      "Cypress": true,
      "cy": true
    },
    "ignorePatterns": [
      "dist/**"
    ]
  }
}
