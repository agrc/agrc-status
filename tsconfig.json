{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@images/*": [
        "src/images/*"
      ],
      "@utils/*": [
        "src/utilities/*"
      ],
      "@models/*": [
        "src/types/*"
      ],
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dist",
    "migrations",
  ]
}
